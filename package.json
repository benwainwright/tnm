    "yaml-lint": "^1.2.4"
  },
  "scripts": {
    "start": "REACT_APP_VERSION_NUMBER=$(git rev-parse --short HEAD) react-scripts start",
    "build:frontend": "REACT_APP_VERSION_NUMBER=$(git rev-parse --short HEAD) react-scripts build",
    "build:backend": "webpack --progress --config webpack.config.backend.js",
    "deploy:frontend": "yarn build:frontend && cdk deploy --require-approval never --all --outputs-file src/frontend-outputs.json",
    "deploy:backend": "yarn build:backend && DO_BACKEND=yes cdk deploy --require-approval never --all --outputs-file src/backend-outputs.json",
    "lint": "eslint .",
    "tsc:watch": "tsc --project ./config/tsconfig.incremental.json --watch",
    "test:noisy": "react-scripts test",
    "test": "yarn test:noisy --silent",
    "start:and-wait": "yarn start & yarn wait-on http://localhost:3000",
    "test:cypress:record": "cypress run --record --reporter mochawesome --reporter-options \"reportDir=cypress/report/mochawesome-report,overwrite=false,html=false,json=true,timestamp=mmddyyyy_HHMMss\"",
    "test:ci": "CI=true react-scripts test --silent --coverage",
    "eject": "react-scripts eject",
    "amplify:CustomGraphqlResolver": "tsc && mv ./dist/amplify/backend/function/CustomGraphqlResolver/ts ./amplify/backend/function/CustomGraphqlResolver/src",
    "build:models": "graphql-codegen --config codegen.yml"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "lint-prepush"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "lint-prepush": {
    "base": "main",
    "tasks": {
      "*.{js,ts,jsx,tsx}": [
        "prettier --write",
        "eslint",
        "react-scripts test --watchAll=false --findRelatedTests"
      ],
      "*.json": [
        "prettier --write",
        "jsonlint"
      ]
    }
  },
  "jest": {
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!grommet-controls)"
    ],
    "coveragePathIgnorePatterns": [
      "src/fixtures",
      "src/API.ts",
      "src/aws-exports.js",
      "src/cdk-entry.ts",
      "src/index.tsx",
      "src/react-app-env.d.ts",
      "src/reportWebVitals.ts",
      "src/setupTests.ts",
      "src/graphql"
    ]
  },
  "eslintConfig": {
    "ignorePatterns": [
      "dist",
      "amplify/**",
      "src/API.ts",
      "template",
      "coverage",
      "build"
    ],
    "extends": "benwainwright",
    "parserOptions": {
      "project": [
        "./tsconfig.json",
        "./cypress/tsconfig.json"
      ]
    },
    "rules": {
      "unicorn/no-null": "off"
    },
    "overrides": [
      {
        "files": [
          "src/infrastructure/**/*"
        ],
        "rules": {
          "no-new": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.19.4",
    "@graphql-codegen/introspection": "1.18.1",
    "@graphql-codegen/typescript": "1.19.0",
    "webpack-cli": "^4.2.0"
  }
}
