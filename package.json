{
  "name": "tnm",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^10.2.1",
    "@testing-library/user-event": "^12.0.2",
    "@types/flux": "^3.1.9",
    "@types/jest": "^25.0.0",
    "@types/jest-when": "^2.7.2",
    "@types/lodash": "^4.14.165",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.6",
    "@types/styled-components": "^5.1.4",
    "EventEmitter": "^1.0.0",
    "cypress": "^5.5.0",
    "eslint": "^7.13.0",
    "eslint-config-benwainwright": "https://github.com/benwainwright/eslint-config-benwainwright",
    "eslint-plugin-array-func": "^3.1.7",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-spellcheck": "^0.0.17",
    "eslint-plugin-unicorn": "^23.0.0",
    "flux": "^3.1.3",
    "fontsource-roboto": "^3.0.3",
    "grommet": "https://github.com/grommet/grommet/tarball/stable",
    "grommet-icons": "^4.5.0",
    "husky": "^4.3.0",
    "jest-mock-extended": "^1.0.10",
    "jest-when": "^3.0.1",
    "lighthouse": "^6.4.1",
    "lint-prepush": "^2.1.0",
    "lint-staged": "^10.5.1",
    "lodash": "^4.17.20",
    "lodash.debounce": "^4.0.8",
    "mocha": "^8.2.1",
    "mochawesome": "^6.2.1",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.1.0",
    "moment": "^2.29.1",
    "polished": "^4.0.3",
    "prettier": "^2.1.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.0",
    "start-server-and-test": "^1.11.5",
    "styled-components": "^5.2.1",
    "ts-jest": "^26.4.4",
    "typescript": "^3.9.5",
    "use-debounce": "^5.1.0",
    "use-deep-compare-effect": "^1.4.0",
    "wait-on": "^5.2.0",
    "web-vitals": "^0.2.2",
    "yaml-lint": "^1.2.4"
  },
  "scripts": {
    "start": "REACT_APP_VERSION_NUMBER=$(git rev-parse --short HEAD) react-scripts start",
    "build": "REACT_APP_VERSION_NUMBER=$(git rev-parse --short HEAD) react-scripts build",
    "lint": "eslint .",
    "test": "react-scripts test",
    "start:and-wait": "yarn start & yarn wait-on http://localhost:3000",
    "test:cypress:record": "cypress run --record --reporter mochawesome --reporter-options \"reportDir=cypress/report/mochawesome-report,overwrite=false,html=false,json=true,timestamp=mmddyyyy_HHMMss\"",
    "test:ci": "CI=true react-scripts test --coverage",
    "eject": "react-scripts eject"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "lint-prepush"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "lint-prepush": {
    "base": "master",
    "tasks": {
      "*.{yml,yaml}": [
        "yamllint"
      ],
      "*.{js,ts,jsx,tsx}": [
        "prettier --write",
        "eslint",
        "react-scripts test --watchAll=false --findRelatedTests"
      ],
      "*.json": [
        "prettier --write",
        "jsonlint"
      ]
    }
  },
  "eslintConfig": {
    "ignorePatterns": [
      "src/aws-exports.js",
      "coverage",
      "build"
    ],
    "extends": "benwainwright",
    "parserOptions": {
      "project": [
        "./tsconfig.json",
        "./cypress/tsconfig.json"
      ]
    },
    "rules": {
      "unicorn/no-null": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
